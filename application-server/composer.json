{
    "name": "virtual-classroom-demo/backend",
    "description": "Virtual Classroom Demo Backend API",
    "type": "project",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^11.0",
        "vlucas/phpdotenv": "^5.6",
        "agence104/livekit-server-sdk": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "authors": [
        {
            "name": "Virtual Classroom Demo Team"
        }
    ],
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S 0.0.0.0:6080 -t public",
        "dev": "php artisan serve --host=0.0.0.0 --port=6080",
        "migrate": "php artisan migrate",
        "post-install-cmd": [
            "php artisan key:generate"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
